{
    "name": "IdS-Codespace",
    "image": "mcr.microsoft.com/vscode/devcontainers/java:0-8",
    "features": {
        "doxygen": "latest",
        "ghcr.io/devcontainers/features/java:1": {
            "installMaven": true
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "vscjava.vscode-java-pack",
                "vscjava.vscode-maven",
                "eamodio.gitlens"
            ]
        }
    }
}